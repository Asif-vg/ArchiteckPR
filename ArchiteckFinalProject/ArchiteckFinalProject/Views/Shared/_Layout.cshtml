@model VmLayout 
<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
    <link rel="stylesheet" href="https://pro.fontawesome.com/releases/v5.8.2/css/all.css" integrity="sha384-xVVam1KS4+Qt2OrFa+VdRUoXygyKIuNWUUUBZYv+n27STsJ7oDOHJgfF0bNKLMJF" crossorigin="anonymous">
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Jost:ital,wght@0,100;0,200;0,300;0,400;0,500;0,600;0,700;0,800;0,900;1,100;1,200;1,300;1,400;1,500;1,600;1,700;1,800&family=Poppins:ital,wght@0,100;0,200;0,300;0,400;0,500;0,600;0,700;0,800;0,900;1,100;1,200;1,300;1,400;1,500;1,600;1,700;1,800;1,900&display=swap"
          rel="stylesheet">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/OwlCarousel2/2.3.4/assets/owl.carousel.min.css">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/OwlCarousel2/2.3.4/assets/owl.theme.default.min.css">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/animate.css/4.1.1/animate.min.css" />
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/odometer.js/0.4.7/themes/odometer-theme-minimal.min.css">
    <link rel="stylesheet" href="https://unpkg.com/aos@next/dist/aos.css" />
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/magnific-popup.js/1.1.0/magnific-popup.css">
    <link rel="icon" type="image/png" href="./img/FaviconLogo/favicon.ico">

    <link rel="stylesheet" href="~/css/style.css">

    <title>Architeck</title>
</head>

<body>
    <div class="page-loader">
        <div class="loading">
            <img src="~/img/Loader/loder.gif" alt="">
        </div>
    </div>

    <header>

        <div class="custom-navs">
            <div class="search-box">
                <form class="forms" asp-controller="Project" asp-action="Index" method="get">
                    <div>
                        @*<label for="" name="search"></label>*@
                        <input asp-for="@Model.Search.searchData" class="form-control search-home" placeholder="Search...">
                        <input asp-for="@Model.Search arcId" hidden />

                    </div>

                    <button type="submit"><i class="far fa-search"></i></button>
                </form>
            </div>
            <div class="container-fluid">
                <div class="row">
                    <div class="col-lg-2 col-md-4">
                        <div class="site-logo">
                            <a class="logo" asp-controller="Home" asp-action="Index"><img src="~/Uploads/@Model.Setting.Logo" alt="Architeck"></a>
                        </div>
                    </div>
                    <div class="col-lg-10 col-md-8">
                        <div class="top-header">
                            <div class="row">
                                <div class="col-lg-3">
                                    <div class="header-social">
                                        <ul>
                                            @foreach (var social in Model.Socials)
                                            {
                                                <li><a target="_blank" href="@social.Link"><i class="@social.Icon"></i> </a></li>
                                            }
                                        </ul>
                                    </div>
                                </div>
                                <div class="col-lg-9">
                                    <div class="contact-header">
                                        <ul>
                                            <li><a href="tel:+1800-001-658"><i class="fas fa-phone"></i><span></span>@Model.Setting.Phone</a></li>
                                            <li><a href="mailto:info@peacefulthemes.com"><i class="fas fa-envelope"></i><span></span>@Model.Setting.Email</a></li>
                                            <li><a href="#"><i class="far fa-clock"></i><span>Mon - Sat 8:00 - 7:30, Sunday - CLOSED</span></a></li>
                                        </ul>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="foot-header">
                            <div class="row">
                                <div class="col-lg-12">
                                    <div class="custom-navbar">
                                        <div class="custom-nav">
                                            <div class="header-page">
                                                <ul>
                                                    <li><a asp-controller="Home" asp-action="Index">Home</a></li>
                                                    <li><a asp-controller="About" asp-action="Index">About</a></li>
                                                    <li><a asp-controller="Project" asp-action="Index">Projects</a></li>
                                                    <li><a asp-controller="Process" asp-action="Index">Process</a></li>
                                                    <li><a asp-controller="Client" asp-action="Index">Clients</a></li>
                                                    <li><a asp-controller="Service" asp-action="Index">Services</a></li>
                                                    <li><a asp-controller="Team" asp-action="Index">Team</a></li>
                                                    <li><a asp-controller="Contact" asp-action="Index">Contact</a></li>
                                                    <li><a asp-controller="Faq" asp-action="Index">FAQ</a></li>

                                                </ul>
                                            </div>
                                        </div>
                                        <div class="header-search">
                                            <a href="#" class="loop-search"><i class="far fa-search"></i></a>

                                            <a href="#" class="x-search"><i class="fal fa-times"></i></i></a>

                                        </div>
                                        <a class="res-menu-bar-lines" href="#">
                                            <span></span>
                                            <span></span>
                                            <span></span>
                                            <p class="innerRes-menu-bar-lines" href="#">X</p>
                                        </a>
                                        <div class="res-nav-menu">
                                            <ul class="nav-menu-ul">
                                                <li><a asp-controller="Home" asp-action="Index">Home</a></li>
                                                <li><a asp-controller="About" asp-action="Index">About</a></li>
                                                <li><a asp-controller="Project" asp-action="Index">Projects</a></li>
                                                <li><a asp-controller="Process" asp-action="Index">Process</a></li>
                                                <li><a asp-controller="Client" asp-action="Index">Clients</a></li>
                                                <li><a asp-controller="Service" asp-action="Index">Services</a></li>
                                                <li><a asp-controller="Team" asp-action="Index">Team</a></li>
                                                <li><a asp-controller="Contact" asp-action="Index">Contact</a></li>
                                                <li><a asp-controller="Faq" asp-action="Index">FAQ</a></li>
                                            </ul>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>

                    </div>
                </div>
            </div>
        </div>
    </header>

    <main>
        @RenderBody()
        <section class="subscribe">
            <div class="container">
                <div class="row">
                    <div class="col-lg-6">
                        <div class="subscribe-context">
                            <div class="subscribe-img">
                                <img class="img-fluid" src="~/img/Home-img/mail.png" alt="">
                            </div>
                            <div class="subscribe-details">
                                <h6>Sign Up to Get Latest Updates</h6>
                                <p>It is a long established fact that a reader will be distracted by the</p>
                            </div>
                        </div>
                    </div>
                    <div class="col-lg-6">
                        <form id="mc4wp-form-1" class="mc4wp-form mc4wp-form-7" asp-controller="home" asp-action="Subscribe" method="post" style="position:relative;">
                            <input type="email" name="email" class="email" autocomplete="off" placeholder="Your email address">
                            <div class="alert alert-success" style="display: none; position: absolute; top: 53px; width: 100%;"></div>
                            <div class="alert alert-warning" style="display: none; position: absolute; top: 53px; width: 100%;"></div>
                            <button type="submit" class="submit" id="submitBtn" value="subscribe">Sign up</button>

                        </form>
                    </div>
                </div>
            </div>
        </section>
    </main>
    <footer>
        <div class="main-footer">
            <div class="row">
                <div class="col-lg-3 col-md-6">
                    <div class="footer-logo">
                        <img class="img-fluid" src="~/Uploads/@Model.Setting.Logo" alt="">
                    </div>
                    <div class="foot-about">
                        <p>@Model.Setting.About</p>
                    </div>
                </div>
                <div class="col-lg-3 col-md-6">
                    <div class="foot-head">
                        <h4>Useful links</h4>
                    </div>
                    <div class="foot-choose">
                        <ul>
                            <li><a asp-controller="Process" asp-action="Index">Our Process</a></li>
                            <li><a asp-controller="Team" asp-action="Index">Our Team</a></li>
                            <li><a asp-controller="Contact" asp-action="Index">Contact Us</a></li>
                            <li><a asp-controller="Client" asp-action="Index">Our Clients</a></li>
                            <li><a asp-controller="About" asp-action="Index">About Us</a></li>
                        </ul>
                    </div>
                </div>
                <div class="col-lg-3 col-md-6">
                    <div class="foot-head">
                        <h4>Our service</h4>
                    </div>
                    <div class="foot-choose">
                        <ul>
                            <li><a href="#">Construction Management</a></li>
                            <li><a href="#">Expert Mechanical</a></li>
                            <li><a href="#">Architecture & Building</a></li>
                            <li><a href="#">Tiling & Painiting</a></li>
                            <li><a href="#">Apartment Design</a></li>
                            <li><a href="#">Facade Engineering</a></li>


                        </ul>
                    </div>
                </div>
                <div class="col-lg-3 col-md-6">
                    <div class="foot-head">
                        <h4>Our Contact </h4>
                    </div>
                    <div class="foot-choose">
                        <ul class="differentCol">
                            <li class="d-flex">
                                <i class="fas fa-map-marker-alt"></i>
                                <span class="mail-foot">@Model.Setting.Address</span>
                            </li>
                            <li>
                                <i class="fas fa-phone"></i>
                                <a href="tel:+1800001658">

                                    <span class="mail-foot">@Model.Setting.Phone</span>
                                </a>
                            </li>
                            <li>
                                <i class="fas fa-envelope"></i>
                                <a href="mailto:info@peacefulthemes.com">

                                    <span class="mail-foot">@Model.Setting.Email</span>
                                </a>
                            </li>
                        </ul>
                    </div>
                </div>
            </div>
        </div>
        <div class="foot-copyright">
            <div class="row">
                <div class="col-md-6">
                    <div class="footer-social">
                        <ul>
                            @foreach (var social in Model.Socials)
                            {
                                <li><a target="_blank" href="@social.Link"><i class="@social.Icon"></i> </a></li>
                            }

                        </ul>
                    </div>
                </div>
                <div class="col-md-4">
                    <a asp-controller="Home" asp-action="Index"><span> Copyright 2020 architeck All RightsReserved.</span></a>
                </div>
                <div class="col-md-2">

                    <span>Created by:<a href="https://github.com/" target="blank">Asif Guluyev</a> </span>
                </div>
            </div>
        </div>
    </footer>
    <div class="circle">
        <i class="far fa-angle-up"></i>
    </div>
</body>

<script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.6.0/jquery.min.js"></script>
<!-- <script src="/path/to/cdn/jquery.min.js"></script> -->

<script src="https://cdnjs.cloudflare.com/ajax/libs/OwlCarousel2/2.3.4/owl.carousel.min.js"></script>
@*<script src="https://cdnjs.cloudflare.com/ajax/libs/odometer.js/0.4.5/odometer.min.js"></script>
*@
<script src="~/increasing-counter-effect/src/countfect.min.js"></script>

<script src="~/isotope/isotope.pkgd.min.js"></script>
<script src="~/magnific-popup/jquery.magnific-popup.min.js"></script>
<script src="~/js/gallery.init.js"></script>


<!-- <script src="/increasing-counter-effect/src/countfect.min.js"></script> -->
<script src="https://unpkg.com/aos@next/dist/aos.js"></script>
<script>
    AOS.init();
</script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/magnific-popup.js/1.1.0/jquery.magnific-popup.js"></script>
<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.min.js" integrity="sha384-QJHtvGhmr9XOIpI6YVutG+2QOK9T+ZnN4kzFN1RtK3zEFEIsxhlmWl5/YESvpZ13" crossorigin="anonymous"></script>
<script src="~/js/script.js"></script>
<script src="~/js/subscribe.js"></script>
</html>